<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Actions</key>
	<array>
		<dict>
			<key>After</key>
			<string>show-result</string>
			<key>Image File</key>
			<string>memo.png</string>
			<key>Long Running</key>
			<true/>
			<key>Script Interpreter</key>
			<string>/usr/bin/php</string>
			<key>Shell Script File</key>
			<string>memo.php</string>
			<key>Title</key>
			<string>Send to Memo</string>
		</dict>
	</array>
	<key>Apps</key>
	<array>
		<dict>
			<key>Link</key>
			<string>https://flomoapp.com</string>
			<key>Name</key>
			<string>flomoapp</string>
		</dict>
	</array>
	<key>Extension Description</key>
	<dict>
		<key>en</key>
		<string>Send the selected text to memo.</string>
		<key>zh-CN</key>
		<string>发成一条 Mome</string>
	</dict>
	<key>Extension Identifier</key>
	<string>com.flomoapp.memo</string>
	<key>Extension Name</key>
	<string>Memo</string>
	<key>Options</key>
	<array>
		<dict>
			<key>Option Description</key>
			<string>Please get the API interface address from flomoapp.com.</string>
			<key>Option Identifier</key>
			<string>api</string>
			<key>Option Label</key>
			<dict>
				<key>en</key>
				<string>API address</string>
				<key>zh-CN</key>
				<string>API 地址</string>
			</dict>
			<key>Option Type</key>
			<string>string</string>
		</dict>
		<dict>
			<key>Option Description</key>
			<string>The tag will be appended to the end of the text.</string>
			<key>Option Identifier</key>
			<string>tag</string>
			<key>Option Label</key>
			<dict>
				<key>en</key>
				<string>Tag</string>
				<key>zh-CN</key>
				<string>附加Tag</string>
			</dict>
			<key>Option Type</key>
			<string>string</string>
		</dict>
	</array>
</dict>
</plist>
